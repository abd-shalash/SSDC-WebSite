
@{
    ViewBag.Title = "AdminProfile";
}

<h2>AdminProfile</h2>

<body>
    <div class="form-group">
      <label for="usrf">First Name:</label>
      <input type="text" class="form-control" id="usrf">
    </div>
    <div class="form-group">
      <label for="usrl">Last Name:</label>
      <input type="text" class="form-control" id="usrl">
    </div>
    <div class="form-group">
      <label for="title">Title:</label>
      <input type="text" class="form-control" id="usrt">
    </div>
    <div class="form-group">
      <label for="usrnum">User Number:</label>
      <input type="text" class="form-control" id="usrno">
    </div>
    <div class="form-group">
      <label for="usrEmail">Email:</label>
      <input type="text" class="form-control" id="usrEmail">
    </div>
    <div class="form-group">
      <label for="pwd">Password:</label>
      <input type="password" class="form-control" id="pwd">
    </div>
        <div class="form-group">
      <label for="pwdcon">Confirm Password:</label>
      <input type="password" class="form-control" id="pwdcon">
    </div>
    <div class="form-group">
        <label for="org">Organization:</label>
        <input type="radio" class="form-control" id="org">PNU</input>
        <input type="radio" class="form-control" id="org2">KAAUH</input>
        <input type="radio" class="form-control" id="others">OTHER</input>
    </div>
</body>