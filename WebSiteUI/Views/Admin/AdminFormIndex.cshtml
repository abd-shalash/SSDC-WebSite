@model IEnumerable<SSDC_WebSite.Models.form>

@{
    ViewBag.Title = "AdminFormIndex";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>AdminFormIndex</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.form_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.form_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.form_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.form_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.form_id })
        </td>
    </tr>
}

</table>
