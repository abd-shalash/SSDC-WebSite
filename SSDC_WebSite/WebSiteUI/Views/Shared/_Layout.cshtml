<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />

    <link href="~/Static/css/webflow.css" rel="stylesheet" type="text/css" />
    <link href="~/Static/css/nassers-stupendous-site.webflow.css" rel="stylesheet" type="text/css" />
    <link href="~/Static/css/normalize.css" rel="stylesheet" type="text/css" />
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.4.7/webfont.js"></script>
</head>
<body>
    <div id="Header" data-collapse="medium" data-animation="default" data-duration="400" data-contain="1" class="w-nav navigation">
    <div class="w-container">
        <a href="~/Static/index.html" class="w-nav-brand brand-link">
            <div class="logo-text">
                Princess Noura University
                <br>Simulation &amp; Skills Development Center
            </div>
        </a>
        <nav role="navigation" class="w-nav-menu nav-menu">
            <a href="~/Static/index.html" class="w-nav-link nav-link">Home</a><a href="#facility" class="w-nav-link nav-link">Facility</a><a href="#events" class="w-nav-link nav-link">Events</a><a href="#calendar" class="w-nav-link nav-link">Calendar</a><a href="~/Static/signup.html" class="w-nav-link nav-link">Sign Up</a><a href="~/Static/login.html" class="w-nav-link nav-link">Login</a>
        </nav>
        <div class="w-nav-button nav-link menu">
            <div class="w-icon-nav-menu"></div>
        </div>
    </div>
</div>


    <div class="container body-content">
        @RenderBody()
        <hr />
        <div id="footer" class="w-section section footer">
            <div class="w-container">
                <div class="w-row">
                    <div class="w-col w-col-6">
                        <div class="footer-title">Princess Noura University</div>
                        <div class="logo-text footer">Simulation &amp; Skills Development Center</div>
                        <div class="footer-slogan">Sign up for our newsletter</div>
                        <div class="w-form newsletter-form">
                            <form id="wf-form-newsletter" name="wf-form-newsletter" data-name="Newsletter" class="w-clearfix">
                                <input id="email" type="email" placeholder="Email Address" name="email" data-name="Email" required="required" class="w-input newsletter-field">
                                <input type="submit" value="Subscribe" data-wait="Wait..." wait="Wait..." class="w-button newsletter-button">
                            </form>
                            <div class="w-form-done success-message">
                                <p>Thank you! You’ll receive an email shortly.</p>
                            </div>
                            <div class="w-form-fail">
                                <p>Oops! Something went wrong while submitting the form :(</p>
                            </div>
                        </div>
                        <div class="social-icon-group">
                            <a href="#" class="w-inline-block social-icon">
                                <img src="~/Static/images/facebook-icon.svg" class="nav-link">
                            </a>
                            <a href="#" class="w-inline-block social-icon">
                                <img src="~/Static/images/twitter-icon.svg" class="nav-link">
                            </a>
                            <a href="#" class="w-inline-block social-icon">
                                <img src="~/Static/images/email-icon-white.svg" class="nav-link">
                            </a>
                        </div>
                    </div>
                    <div class="w-col w-col-6">
                        <div>
                            Email
                            <br>kaauh-simulation@pnu.edu.sa
                            <br>Telephone
                            <br>+966 11 8239733
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>